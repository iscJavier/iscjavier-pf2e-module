{
  "name": "iscjavier-pf2e-module",
  "title": "iscjavier PF2e module",
  "version": "0.0.1",
  "description": "module for PF2e",
  "main": "src/main.ts",
  "scripts": {
    "build": "npx gulp",
    "devbuild": "npx gulp devbuild",
    "compress": "npx gulp compress",
    "tag": "git tag %npm_package_name%_%npm_package_version% && git push origin %npm_package_name%_%npm_package_version%"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "overrides": [
      {
        "files": "**/*.{hbs}",
        "options": {
          "parser": "glimmer"
        }
      }
    ]
  },
  "devDependencies": {
    "@league-of-foundry-developers/foundry-vtt-types": "^9.280.0",
    "@types/jquery": "^3.5.30",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-tabify": "0.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-zip": "^5.0.2",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "^5.5.4"
  }
}
